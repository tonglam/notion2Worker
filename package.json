{
  "name": "notion-to-r2",
  "version": "1.0.0",
  "description": "A lightweight tool that retrieves a database from Notion, transforms it into a JSON file, and uploads it to Cloudflare R2 storage, running on a Cloudflare Worker",
  "main": "worker.js",
  "type": "module",
  "scripts": {
    "start": "node notion-to-r2.js",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "notion",
    "cloudflare",
    "r2",
    "storage",
    "markdown",
    "blog",
    "worker"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@notionhq/client": "^2.2.13",
    "notion-to-md": "^3.1.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "nodemon": "^3.0.1",
    "wrangler": "^3.22.0"
  },
  "engines": {
    "node": ">=16.13.0"
  }
}
